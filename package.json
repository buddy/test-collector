{
  "name": "test-collector",
  "version": "0.9.0",
  "description": "Universal test results collector for Jest, Jasmine, Mocha, Cypress, Playwright, and Vitest that sends results to Buddy Works API in real-time",
  "keywords": [
    "testing",
    "jest",
    "jasmine",
    "mocha",
    "cypress",
    "playwright",
    "vitest",
    "test-reporter",
    "buddy",
    "buddy-works",
    "ci",
    "ci-cd"
  ],
  "homepage": "https://github.com/buddy-works/test-collector#readme",
  "bugs": "https://github.com/buddy-works/test-collector/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buddy-works/test-collector.git"
  },
  "license": "MIT",
  "exports": {
    "./jest": "./dist/reporters/jest/index.js",
    "./jasmine": "./dist/reporters/jasmine/index.js",
    "./mocha": "./dist/reporters/mocha/index.js",
    "./cypress": "./dist/reporters/cypress/index.js",
    "./playwright": "./dist/reporters/playwright/index.js",
    "./vitest": "./dist/reporters/vitest/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "1.10.0"
  },
  "devDependencies": {
    "typescript": "5.8.3",
    "@types/node": "14"
  },
  "peerDependencies": {
    "jest": ">=24.0.0",
    "jasmine": ">=3.0.0",
    "mocha": ">=10.0.0",
    "cypress": ">=10.0.0",
    "playwright": ">=1.0.0",
    "vitest": ">=0.34.0"
  },
  "peerDependenciesMeta": {
    "jest": {
      "optional": true
    },
    "jasmine": {
      "optional": true
    },
    "mocha": {
      "optional": true
    },
    "cypress": {
      "optional": true
    },
    "playwright": {
      "optional": true
    },
    "vitest": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
